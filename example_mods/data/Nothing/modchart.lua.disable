	if g == nil then g = 0 end
	if b == nil then b = 0 end
	rgb = {r,g,b}
	makeLuaSprite(tag..'BALLS420','',x,y)
	makeGraphic(tag..'BALLS420',width,height,rgbToHex(rgb))
end

